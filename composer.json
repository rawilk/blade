{
    "name": "rawilk/blade",
    "description": "Blade components for Laravel.",
    "keywords": [
        "rawilk",
        "laravel",
        "blade"
    ],
    "homepage": "https://github.com/rawilk/blade",
    "license": "MIT",
    "authors": [
        {
            "name": "Randall Wilk",
            "email": "randall@randallwilk.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.13"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "spatie/laravel-ray": "^1.31"
    },
    "autoload": {
        "psr-4": {
            "Rawilk\\Blade\\": "src",
            "Rawilk\\Blade\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rawilk\\Blade\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rawilk\\Blade\\BladeServiceProvider"
            ],
            "aliases": {
                "Blade": "Blade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}